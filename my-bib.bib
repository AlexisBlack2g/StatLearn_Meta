	Issn = {1939-1285, 0278-7393},
	Journaltitle = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
	Langid = {english},
	Number = {2},
	Pages = {221--232},
	Shorttitle = {The longevity of statistical learning},
	Title = {The longevity of statistical learning: When infant memory decays, isolated words come to the rescue.},
	Url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/xlm0000448},
	Urldate = {2018-11-21},
